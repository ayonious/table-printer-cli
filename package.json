{
  "name": "table-printer-cli",
  "version": "0.0.0",
  "description": "console table printer cli. Print table on bash terminal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "ctp": "dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "setup": "npm install",
    "build": "tsc",
    "lint": "eslint --ext=js,ts .",
    "format": "prettier --write \"**/*.{json,md,ts,tsx,yml,js,jsx}\"",
    "test": "jest --config jestconfig.json",
    "semantic-release": "semantic-release"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ayonious/table-printer-cli.git"
  },
  "keywords": [
    "console-table",
    "console-log",
    "print-table",
    "cli"
  ],
  "author": "Nahiyan Kamal",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ayonious/table-printer-cli/issues"
  },
  "homepage": "https://console-table.netlify.app/docs/doc-cli-install-quick-start",
  "dependencies": {
    "commander": "^12.1.0",
    "console-table-printer": "^2.12.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.0",
    "@typescript-eslint/eslint-plugin": "^8.11.0",
    "@typescript-eslint/parser": "^8.11.0",
    "eslint": "^9.13.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.31.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "pretty-quick": "^4.0.0",
    "semantic-release": "^24.1.3",
    "ts-jest": "^29.2.5",
    "typescript": "^5.6.3"
  }
}
